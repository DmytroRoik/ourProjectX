{
  "name": "ProjectX",
  "version": "1.0.0",
  "description": "our version of poll app",
  "repository": {
    "type": "git",
    "url": "https://github.com/redeyeowner/ourProjectX.git"
  },

  "author": "redeyeowner & DmytroRoik",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "path": "^0.12.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "webpack": "^1.12.12"
  }
}
